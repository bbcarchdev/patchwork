# Extends integration.yml.default; run with `docker-compose -f integration.yml.default -f development.yml.default ...`

twine:
  build: ../../twine/
  dockerfile: docker/Dockerfile-build
  volumes:
   - ../twine:/usr/local/src-spindle

quilt:
  build: ../../quilt/
  dockerfile: docker/Dockerfile-build
  ports:
   - "80:80"
  volumes:
   - ../spindle:/usr/local/src-spindle
